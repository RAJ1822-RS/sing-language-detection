<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude app preferences from cloud backup -->
        <exclude domain="sharedpref" path="onboarding_preferences.preferences_pb" />
    </cloud-backup>
    <device-transfer>
        <!-- Include app preferences in device transfer -->
        <include domain="sharedpref" path="." />
    </device-transfer>
</data-extraction-rules>